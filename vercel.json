{
  "version": 2,
  "builds": [
    {
      "src": "public/assets/**",
      "use": "@vercel/static"
    },
    {
      "src": "public/uploads/**",
      "use": "@vercel/static"
    },
    {
      "src": "public/index.php",
      "use": "vercel-php@0.7.1",
      "config": {
        "includeFiles": ["includes/**", "database/**"]
      }
    },
    {
      "src": "public/admin/*.php",
      "use": "vercel-php@0.7.1",
      "config": {
        "includeFiles": ["includes/**", "database/**"]
      }
    },
    {
      "src": "public/**/*.php",
      "use": "vercel-php@0.7.1",
      "config": {
        "includeFiles": ["includes/**", "database/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/public/assets/$1"
    },
    {
      "src": "/uploads/(.*)",
      "dest": "/public/uploads/$1"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/public/admin/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.php"
    }
  ],
  "env": {
    "SITE_NAME": "TTG Pondokrejo"
  }
}
